<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.EntrarController">
   <children>
      <Label alignment="TOP_LEFT" layoutX="19.0" layoutY="45.0" text="Jogo Da Forca" textFill="#0873f5">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" alignment="CENTER" layoutX="147.0" layoutY="47.0" promptText="Username" />
      <Button fx:id="iniciarjogo" alignment="CENTER" layoutX="100.0" layoutY="92.0" mnemonicParsing="false" onAction="#abrirmododejogo" text="INICIAR JOGO" />
      <Button fx:id="classificacao" alignment="CENTER" layoutX="100.0" layoutY="130.0" mnemonicParsing="false" onAction="#abrirclassificacao" text="CLASSIFICAÇÃO" />
      <Button fx:id="sair" alignment="CENTER" layoutX="100.0" layoutY="168.0" mnemonicParsing="false" onAction="#sair" text="SAIR" />
   </children>
</AnchorPane>
